{
  "private": true,
  "keywords": [
    "google",
    "javascript",
    "maps",
    "samples"
  ],
  "scripts": {
    "turbo-dev": "next dev --turbo",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,md}\"",
    "debug": "NODE_OPTIONS='--inspect' next"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "none",
    "tabWidth": 2,
    "singleQuote": true,
    "jsxBracketSameLine": false,
    "printWidth": 180,
    "arrowParens": "avoid"
  },
  "babel": {
    "plugins": [
      [
        "babel-plugin-styled-components"
      ]
    ]
  },
  "dependencies": {
    "@react-google-maps/api": "2.18.1",
    "clsx": "1.2.1",
    "next": "latest",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "styled-components": "6.0.0-beta.2",
    "styled-jsx": "5.1.0",
    "turbo": "1.7.3",
    "use-count-up": "3.0.1"
  },
  "devDependencies": {
    "@types/google-map-react": "2.1.7",
    "@types/google.maps": "3.51.1",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "autoprefixer": "^10.4.7",
    "babel-plugin-styled-components": "2.0.7",
    "eslint": "8.19.0",
    "eslint-config-next": "latest",
    "eslint-plugin-import": "2.27.5",
    "postcss": "^8.4.14",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "typescript": "4.9.5"
  }
}
