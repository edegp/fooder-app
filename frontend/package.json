{
  "private": true,
  "keywords": [
    "google",
    "javascript",
    "maps",
    "samples"
  ],
  "scripts": {
    "dev": "open http://localhost:3000 && next dev",
    "dev:turbo": "open http://localhost:3000 && next dev --turbo",
    "dev:remote": " open http://localhost:3000 &&  env-cmd -f ./.env.production next dev",
    "build": "next build && next export",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "yarn lint --fix",
    "format": "prettier --write \"src/**/*.{js,ts,tsx,md}\"",
    "debug": "NODE_OPTIONS='--inspect' next",
    "codegen": "graphql-codegen --config codegen.ts"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "babel-plugin-styled-components",
        {
          "ssr": true,
          "displayName": true
        }
      ]
    ]
  },
  "dependencies": {
    "@react-google-maps/api": "2.18.1",
    "@splidejs/react-splide": "0.7.12",
    "clsx": "1.2.1",
    "date-fns": "2.29.3",
    "firebase": "9.17.1",
    "graphql": "16.6.0",
    "graphql-tag": "2.12.6",
    "next": "13.1.7-canary.6",
    "next-intercept-stdout": "1.0.1",
    "next-urql": "4.0.3",
    "react": "18.3.0-next-55542bc73-20230210",
    "react-dom": "18.3.0-next-55542bc73-20230210",
    "react-icons": "4.7.1",
    "react-is": "18.2.0",
    "react-loader-spinner": "5.3.4",
    "recoil": "0.7.6",
    "recoil-sync": "0.2.0",
    "recoil-sync-next": "0.0.4",
    "style-jsx": "0.0.0",
    "styled-components": "6.0.0-beta.2",
    "swr": "2.0.3",
    "turbo": "1.7.3",
    "urql": "3.0.3"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "3.2.0",
    "@graphql-codegen/client-preset": "2.1.0",
    "@graphql-codegen/typescript": "3.0.1",
    "@graphql-codegen/typescript-operations": "3.0.1",
    "@types/google-map-react": "2.1.7",
    "@types/google.maps": "3.51.1",
    "@types/gtag.js": "0.0.12",
    "@types/node": "18.0.3",
    "@types/react": "18.0.15",
    "@types/react-dom": "18.0.6",
    "@types/styled-components": "5.1.26",
    "@typescript-eslint/eslint-plugin": "5.50.0",
    "@typescript-eslint/parser": "5.50.0",
    "autoprefixer": "^10.4.7",
    "babel-plugin-styled-components": "2.0.7",
    "env-cmd": "10.1.0",
    "eslint": "8.19.0",
    "eslint-config-next": "latest",
    "eslint-config-prettier": "8.6.0",
    "eslint-import-resolver-typescript": "3.5.3",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-no-relative-import-paths": "1.5.2",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-tailwindcss": "3.8.3",
    "eslint-plugin-unused-imports": "2.0.0",
    "postcss": "^8.4.14",
    "postcss-nested": "6.0.0",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.12",
    "tailwindcss": "^3.1.6",
    "ts-node": "10.9.1",
    "typescript": "4.9.5"
  }
